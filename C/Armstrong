#include<stdio.h>
int main()
{
    int num,a,i,sum=0;
    printf("Enter any number to check armstrong or not:");
    scanf("%d",&num);
    i=num;
    while(num>0)
    {
        a=num%10;
        sum=sum+(a*a*a);
        num=num/10;

    }
    if(sum==i)
    {
        printf("Number is Armstrong number!!");
    }
    else
    {
        printf("Number is Not Armstrong number!!"); 
        return 0;
    }
}
